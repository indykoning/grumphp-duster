{
    "name": "indykoning/grumphp-duster",
    "description": "A Duster task for GrumPHP.",
    "type": "plugin",
    "keywords": [
        "indykoning",
        "laravel",
        "grumphp",
        "quality",
        "phpcsfixer",
        "pint",
        "duster",
        "linter"
    ],
    "license": "MIT",
    "require": {
        "phpro/grumphp": "^2.0.0",
        "tightenco/duster": "*"
    },
    "autoload": {
        "psr-4": {
            "Indykoning\\GrumPHPDuster\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
